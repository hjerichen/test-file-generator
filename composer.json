{
    "name": "hjerichen/test-file-generator",
    "description": "Builds PHP Unit Test Files for specific PHP Class Files.",
    "type": "testing",
    "license": "MIT",
    "authors": [
        {
            "name": "Heiko Jerichen",
            "email": "heiko@jerichen.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpunit/php-text-template": "^2.0",
        "hjerichen/collections": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.10",
        "psalm/plugin-phpunit": "^0.16.1",
        "symfony/filesystem": "^6.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "hjerichen/prophecy-php": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HJerichen\\TestFileGenerator\\": "src",
            "HJerichen\\TestFileGenerator\\Test\\": "tests"
        }
    }
}
